  "description": "Angular JSON Schema Form builder Demo",
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.0"
  },
  "keywords": [
    "Angular",
    "JSON Schema",
    "form builder"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/alekart/angular-json-schema-form.git",
    "directory": "packages/angular-json-schema-form"
  },
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build-lib-dev",
    "start": "ng serve",
    "start-prod": "ng serve --prod",
    "build-lib": "ng build angular6-json-schema-form --prod",
    "build-lib-dev": "ng build angular6-json-schema-form",
    "postbuild-lib": "cp README.md LICENSE  dist/angular-json-schema-form/",
