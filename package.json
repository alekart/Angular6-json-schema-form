{
  "name": "@alekart/ajsf-demo",
  "version": "1.0.0",
  "author": "https://github.com/alekart/ajsf/graphs/contributors",
  "description": "Angular JSON Schema Form builder Demo",
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.0"
  },
  "keywords": [
    "Angular",
    "JSON Schema",
    "form builder"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://github.com/alekart/ajsf.git",
    "directory": "packages/ajsf"
  },
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build-libs",
    "start": "ng serve",
    "start-prod": "ng serve --prod",
    "build-core": "ng build ajsf-core",
    "build-bs4": "ng build ajsf-bootstrap4",
    "build-bs3": "ng build ajsf-bootstrap3",
    "build-material": "ng build ajsf-material",
    "build-core-dev": "ng build ajsf-core",
    "build-libs": "yarn build-core && yarn build-bs4 && yarn build-bs3 && yarn build-material",
    "postbuild-core": "cp README.md LICENSE  dist/core/",
    "buildDemo": "yarn build-libs && ng build --prod --base-href ./",
    "release": "run-p release:*",
    "release:jsf": "cd ./dist/core && npm publish --access public && cd -",
    "release:bs3": "cd ./dist/bootstrap3 && npm publish && cd -",
    "release:bs4": "cd ./dist/bootstrap4 && npm publish && cd -",
    "release:material": "cd ./dist/material && npm publish && cd -",
    "pack-libs": "run-p pack-lib:*",
    "pack-lib:jsf": "cd ./dist/core && npm pack && cd -",
    "pack-lib:bs3": "cd ./dist/bootstrap3 && npm pack && cd -",
    "pack-lib:bs4": "cd ./dist/bootstrap4 && npm pack && cd -",
    "pack-lib:material": "cd ./dist/material && npm pack && cd -",
    "bump-version": "node ./scripts/bump-version.js",
    "test:core": "ng test ajsf-core",
    "test:bs3": "ng test ajsf-bootstrap3",
    "test:bs4": "ng test ajsf-bootstrap4",
    "test:material": "ng test ajsf-material",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/router": "^8.1.2",
    "brace": "^0.11.1",
    "core-js": "^3.1.4",
    "lodash-es": "^4.17.14",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.136",
    "@types/node": "~12.6.8",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.3"
  }
}
