{
  "name": "@alekart/ajsf-demo",
  "version": "1.0.0-snapshot-0807-efc762a5",
  "author": "https://github.com/alekart/ajsf/graphs/contributors",
  "description": "Angular JSON Schema Form builder Demo",
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.0"
  },
  "keywords": [
    "Angular",
    "JSON Schema",
    "form builder"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://github.com/alekart/ajsf.git",
    "directory": "packages/ajsf"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "ng serve --prod",
    "build-core": "ng build ajsf-core",
    "build-bs4": "ng build ajsf-bootstrap4",
    "build-bs3": "ng build ajsf-bootstrap3",
    "build-material": "ng build ajsf-material",
    "build-core-dev": "ng build ajsf-core",
    "build-libs": "yarn build-core && yarn build-bs4 && yarn build-bs3 && yarn build-material",
    "postbuild-core": "cp README.md LICENSE  dist/core/",
    "buildDemo": "yarn build-libs && ng build --prod --base-href ./",
    "build-snapshot": "run-s set-snap-version build-libs pack-libs && rm -rf ./snapshots || : && mkdir snapshots || : && cp $(find ./dist/ -name '*.tgz') ./snapshots/",
    "release": "run-p release:*",
    "release:jsf": "cd ./dist/core && npm publish --access public && cd -",
    "release:bs3": "cd ./dist/bootstrap3 && npm publish && cd -",
    "release:bs4": "cd ./dist/bootstrap4 && npm publish && cd -",
    "release:material": "cd ./dist/material && npm publish && cd -",
    "pack-libs": "run-p pack-lib:*",
    "pack-lib:jsf": "cd ./dist/core && npm pack && cd -",
    "pack-lib:bs3": "cd ./dist/bootstrap3 && npm pack && cd -",
    "pack-lib:bs4": "cd ./dist/bootstrap4 && npm pack && cd -",
    "pack-lib:material": "cd ./dist/material && npm pack && cd -",
    "bump-version": "node ./scripts/bump-version.js",
    "set-snap-version": "run-s 'bump-version snap,date,hash -- --nocommit'",
    "test:core": "ng test ajsf-core",
    "test:bs3": "ng test ajsf-bootstrap3",
    "test:bs4": "ng test ajsf-bootstrap4",
    "test:material": "ng test ajsf-material",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/flex-layout": "7.0.0-beta.24",
    "@angular/forms": "^7.2.16",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "brace": "^0.11.1",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "rxjs": "^6.6.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.10",
    "@angular-devkit/build-ng-packagr": "~0.13.10",
    "@angular/cli": "^7.3.10",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.136",
    "@types/node": "~12.6.8",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.18.0",
    "typescript": "~3.2.4",
    "yargs": "^15.4.1"
  }
}
