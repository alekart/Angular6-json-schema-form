{
  "name": "@alekart/ajsf-demo",
  "version": "2.0.1-snapshot",
  "author": "https://github.com/alekart/ajsf/graphs/contributors",
  "description": "Angular JSON Schema Form builder Demo",
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.0"
  },
  "keywords": [
    "Angular",
    "JSON Schema",
    "form builder"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://github.com/alekart/ajsf.git",
    "directory": "packages/ajsf"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "ng serve --prod",
    "build:core": "ng build ajsf-core",
    "build:bs4": "ng build ajsf-bootstrap4",
    "build:bs3": "ng build ajsf-bootstrap3",
    "build:material": "ng build ajsf-material",
    "build-core-dev": "ng build ajsf-core",
    "build-libs": "run-s build:*",
    "postbuild-core": "cp README.md LICENSE  dist/core/",
    "buildDemo": "yarn build-libs && ng build --prod --base-href ./",
    "build-snapshot": "run-s set-snap-version build-libs pack-libs && rm -rf ./snapshots || : && mkdir snapshots || : && cp $(find ./dist/ -name '*.tgz') ./snapshots/",
    "release": "run-p release:*",
    "release:core": "cd ./dist/core && npm publish --access public && cd -",
    "release:bs3": "cd ./dist/bootstrap3 && npm publish --access public && cd -",
    "release:bs4": "cd ./dist/bootstrap4 && npm publish --access public && cd -",
    "release:material": "cd ./dist/material && npm publish --access public && cd -",
    "pack-libs": "run-p pack-lib:*",
    "pack-lib:jsf": "cd ./dist/core && npm pack && cd -",
    "pack-lib:bs3": "cd ./dist/bootstrap3 && npm pack && cd -",
    "pack-lib:bs4": "cd ./dist/bootstrap4 && npm pack && cd -",
    "pack-lib:material": "cd ./dist/material && npm pack && cd -",
    "bump-version": "node ./scripts/bump-version.js",
    "set-snap-version": "run-s 'bump-version snap,date,hash -- --nocommit'",
    "test:core": "ng test ajsf-core",
    "test:bs3": "ng test ajsf-bootstrap3",
    "test:bs4": "ng test ajsf-bootstrap4",
    "test:material": "ng test ajsf-material",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "brace": "^0.11.1",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "rxjs": "^6.6.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@types/ajv": "^1.0.0",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.159",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^9.1.5",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "ts-node": "^8.10.2",
    "tslint": "~5.18.0",
    "typescript": "~3.8.3",
    "yargs": "^15.4.1"
  }
}
