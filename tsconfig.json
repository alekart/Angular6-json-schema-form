{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "@alekart/angular-json-schema-form": [
        "dist/angular-json-schema-form"
      ],
      "@alekart/angular-json-schema-form/*": [
        "dist/angular-json-schema-form/*"
      ],
      "ajsf-bootstrap4-framework": [
        "dist/ajsf-bootstrap4-framework"
      ],
      "ajsf-bootstrap4-framework/*": [
        "dist/ajsf-bootstrap4-framework/*"
      ]
    }
  }
}
